{
  "name": "github-adapter.dapplet-base.eth",
  "version": "0.1.4",
  "description": "Site specific adapter for GitHub",
  "author": "Dapplets Project",
  "homepage": "https://dapplets.org",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "start": "rollup -w --config rollup.config.js",
    "build": "rollup --config rollup.config.js",
    "lint": "npm run prettify && npm run stylelint && npm run eslint",
    "prettify": "prettier --write ./packages",
    "eslint": "eslint ./packages --ext .ts --fix",
    "stylelint": "stylelint ./packages/**/*.{css,scss,module.scss} --fix"
  },
  "dependencies": {
    "lit": "^2.4.1"
  },
  "devDependencies": {
    "@dapplets/dapplet-extension": "develop",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-image": "^3.0.1",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.0",
    "rollup": "^3.4.0",
    "rollup-plugin-serve": "^2.0.1",
    "stylelint": "^14.15.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  }
